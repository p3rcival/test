{
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./src/*"]
    },
    "typeRoots": ["src/types", "node_modules/@types"]
  },
  "include": ["./app", "./src", "./src/types", "./types", "./components", "./utils", "./lib", "./hooks", "./assets", "./sounds", "declarations.d.ts",],
  "exclude": ["node_modules"]
}